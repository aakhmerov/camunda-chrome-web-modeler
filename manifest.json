{
  "manifest_version": 2,

  "name": "camunda-chrome-web-modeler",
  "description": "Extension to open attached .bpmn files in web modeler",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
